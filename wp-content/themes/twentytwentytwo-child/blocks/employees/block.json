{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "create-block/tmy-employees",
	"version": "0.1.0",
	"title": "employees",
	"category": "text",
	"icon": "flag",
	"description": "WP plugin for displaying custom-post type gutenberg blocks",
	"attributes": {
		"employees": {
			"type": "array",
			"default": []
		},
		"hascircle": {
			"type": "boolean",
			"default": false
		},
		"direction": {
			"type": "string",
			"default": "left",
			"enum": [
				"left",
				"right"
			]
		}
	},
	"supports": {
		"html": false,
		"align": true,
		"anchor": true
	},
	"textdomain": "tmy",
	"editorScript": "file:../../build/employees.js",
	"editorStyle": "file:../../build/employees.css",
	"style": "file:../../build/style-employees.css"
}