{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "create-block/tmy-circletext",
	"version": "0.1.0",
	"title": "Circular text",
	"category": "text",
	"icon": "marker",
	"description": "Write circular text that spins while scrolling",
	"attributes": {
		"circletext": {
			"type": "string",
			"default": null
		},
		"circlesubtext": {
			"type": "string",
			"default": null
		},
		"direction": {
			"type": "string",
			"default": "left",
			"enum": [
				"left",
				"right"
			]
		},
		"speed": {
			"type": "number",
			"default": 0
		}
	},
	"supports": {
		"html": false,
		"align": true,
		"anchor": true
	},
	"textdomain": "tmy",
	"editorScript": "file:../../build/circletext.js",
	"editorStyle": "file:../../build/circletext.css",
	"style": "file:../../build/style-circletext.css",
	"script": "file:../../build/circletext_frontend.js"
}