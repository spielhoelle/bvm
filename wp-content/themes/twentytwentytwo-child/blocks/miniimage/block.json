{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "create-block/tmy-miniimage",
	"version": "0.1.0",
	"title": "miniimage",
	"category": "text",
	"icon": "flag",
	"description": "WP plugin for displaying custom-post type gutenberg blocks",
	"attributes": {
		"miniImage": {
			"type": "string",
			"default": null
		},
		"size": {
			"type": "number",
			"default": 100
		},
		"direction": {
			"type": "string",
			"default": "left-top",
			"enum": [
				"left-top",
				"right-top",
				"right-bottom",
				"left-bottom"
			]
		}
	},
	"supports": {
		"html": false,
		"align": true,
		"anchor": true
	},
	"textdomain": "tmy",
	"editorScript": "file:../../build/miniimage.js",
	"editorStyle": "file:../../build/miniimage.css",
	"style": "file:../../build/style-miniimage.css"
}